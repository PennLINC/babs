processing_level: '{{ processing_level }}'
queue: '{{ queue }}'

# input dataset's name(s)
input_ds:
{% for i in range(input_ds.num_ds) %}
  $INPUT_DATASET_#{{ i + 1 }}:
    name: '{{ input_ds._datasets[i].name }}'
    path_in: '{{ input_ds._datasets[i].origin_url }}'
    unzipped_path_containing_subject_dirs: 'TO_BE_FILLED'
    is_zipped: 'TO_BE_FILLED'
{% endfor %}

# container ds:
container:
  name: '{{ container_name }}'
  path_in: '{{ container_ds }}'